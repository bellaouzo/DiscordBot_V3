{
  "name": "discord-bot-v3",
  "version": "1.0.0",
  "description": "A clean Discord bot built with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "b": "tsc",
    "s": "node dist/index.js",
    "clean": "rimraf dist",
    "lint": "tsc --noEmit && npm run lint:eslint",
    "lint:eslint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^12.5.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "module-alias": "^2.2.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^2.1.0",
    "baseline-browser-mapping": "^2.9.19",
    "cross-env": "^10.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vitest": "^0.5.4",
    "prettier": "^3.7.4",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "typescript-eslint": "^8.54.0",
    "vitest": "^2.1.0"
  },
  "_moduleAliases": {
    "@commands": "dist/Commands",
    "@systems": "dist/Systems",
    "@middleware": "dist/Commands/Middleware",
    "@bot": "dist/Bot",
    "@database": "dist/Database",
    "@utilities": "dist/Utilities",
    "@shared": "dist/Shared",
    "@responders": "dist/Responders",
    "@events": "dist/Events",
    "@config": "dist/Config"
  }
}
