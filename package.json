{
  "name": "discord-bot-v3",
  "version": "1.0.0",
  "description": "A clean Discord bot built with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "b": "tsc",
    "s": "node dist/index.js",
    "clean": "rimraf dist",
    "lint": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [
    "discord",
    "bot",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^12.5.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "module-alias": "^2.2.3"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.10.0",
    "prettier": "^3.7.4",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "_moduleAliases": {
    "@commands": "dist/Commands",
    "@middleware": "dist/Commands/Middleware",
    "@bot": "dist/Bot",
    "@database": "dist/Database",
    "@utilities": "dist/Utilities",
    "@shared": "dist/Shared",
    "@responders": "dist/Responders",
    "@events": "dist/Events",
    "@config": "dist/Config"
  }
}
